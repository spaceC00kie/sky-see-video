{
  "firestore": {
    "target": "sky-see-video",
    "rules": "firestore.rules"
  },
  "hosting": [
    {
      "target": "sky-see-video",
      "predeploy": "yarn build",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
